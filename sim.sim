# lex skeleton

next = dir@next

files = [
	MODULE+MODULE
	~MODULE+.sim
	MODULE+.log
	ypp.ypp
	lpp.lpp
	hpp.hpp
	next+cpp.cpp
	next+Makefile
]

skeleton = [
	files
	bat.bat
	.gitignore
]

# multi assignment test

[ A B C ]
A = B		# first assignment
[ A B C ]
A = C		# second assignment evaluates as B = C ( A evals-> B )
[ A B C ]
~A = C		# forced second assignment with blocked ~A ( A evals-> A )
[ A B C ]

